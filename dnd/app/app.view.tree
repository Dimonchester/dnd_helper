$my_dnd_app $mol_view
	sub /
		<= Title $mol_text
			text @ \D&D Helper
		<= Start $mol_row
			sub / 
				<= Title_roll $mol_text
					text @ \D&D Dice
				<= Dice_d4 $my_dnd_dice
					sides 4
				<= Dice_d6 $my_dnd_dice
					sides 6
				<= Dice_d8 $my_dnd_dice
					sides 8
				<= Dice_d10 $my_dnd_dice
					sides 10
				<= Dice_d12 $my_dnd_dice
					sides 12
				<= Dice_d20 $my_dnd_dice
					sides 20
				<= Dice_d100 $my_dnd_dice
					sides 100
